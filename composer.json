{
    "name": "zeknoss/firebase-broadcast-driver",
    "description": "a firebase broadcast driver for laravel (fork of ctf0 with some improvements)",
    "homepage": "https://github.com/zeknoss/laravel-firebase-broadcast-driver",
    "license": "MIT",
    "keywords": [
        "ctf0",
        "zeknoss",
        "laravel",
        "firebase",
        "broadcast"
    ],
    "authors": [
        {
            "name": "Muah",
            "email": "muah003@gmail.com"
        },
        {
            "name": "Burak Mehmet Gurbuz",
            "email": "burak@burakgurbuz.com"
        }
    ],
    "require": {
        "illuminate/support": ">=5.5 <10.0",
        "kreait/firebase-php": "*"
    },
    "suggest": {
        "morrislaptop/firestore-php": "Required to use with firestore db."
    },
    "autoload": {
        "psr-4": {
            "zeknoss\\Firebase\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "zeknoss\\Firebase\\FireBaseBroadcastProvider"
            ]
        },
        "changeLog": "logs"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "post-package-install": [
            "@php artisan vendor:publish --provider = \"zeknoss\\Firebase\\FireBaseBroadcastProvider\""
        ]
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/ctf0"
        }
    ]
}
